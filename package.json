{
  "name": "@akshat33/react-user-management",
  "private": false,
  "version": "1.0.0",
  "description": "React User Management System with CRUD Operations",
  "type": "commonjs",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "server": "json-server --watch db.json --port 3001 --host localhost --middlewares ./node_modules/json-server-reset",
    "build": "vite build",
    "build:lib": "vite build",
    "prepublishOnly": "npm run build:lib",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "docs": "typedoc"
  },
  "dependencies": {
    "json-server": "^1.0.0-alpha.23",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-toastify": "^10.0.4"
  },
  "devDependencies": {
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "vite": "^5.1.4",
    "typescript": "^5.0.0",
    "typedoc": "^0.25.0",
    "typedoc-plugin-markdown": "^3.15.0",
    "@types/node": "^18.0.0"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "keywords": [
    "react",
    "user-management",
    "crud",
    "typescript"
  ],
  "author": "Akshat",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akshat33/react-user-management.git"
  }
}
